$("#client_showcase_box ul li").mouseover(function() {
    $(this).find(".album_title").stop().slideToggle()
})
$("#client_showcase_box ul li").mouseout(function() {
    $(this).find(".album_title").stop().slideToggle()
})
for (var i = 0; i < $("#client_showcase_box ul li").length; i++) {
    $("#client_showcase_box ul li").index() = i
};