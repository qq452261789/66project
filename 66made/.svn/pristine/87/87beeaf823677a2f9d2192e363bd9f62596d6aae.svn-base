#header{
	width: 100%;
	color: #605e5e;
}
#header header{
	padding-top: 70px;
	margin: 0 auto ;
	text-align: center;
	font-size: 30px;	
}
#header .small_title{
	padding-top: 15px;
	margin: 0 auto;
	font-size: 16px;
	text-align: center;
}
.show_ul{
	padding: 60px 0px;
	width: 920px;
	margin: 0 auto;
}
.show_ul li{
	width: 286px;
	float: left;
	font-size: 16px;
	color: #2f2e2e;
	text-align: center;
	line-height: 30px;
	margin-right: 30px;
	cursor: pointer;
}
.show_ul .margin_right{
	margin-right: 0px;
}
.img_box{
	width: 286px;
	height: 286px;	
}
.img_box img{
	display: block;
	width: 100%;
	height: 100%;	
}
.img_them{
	margin-top: 15px;
}
.img_time{
	padding-bottom: 30px;
}









