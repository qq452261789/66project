$(document).ready(function(){
    $(".service_list>li").bind("mouseenter",function(){
        $(this).find(".service_img").stop().slideUp();
    })
    $(".service_list>li").bind("mouseleave",function(){
        $(this).find(".service_img").stop().slideDown();
    })
