#about_box{
	width: 100%;
	background: #f1f1f1;
	height: 810px;
}
#about{
	height: 810px;
	width: 980px;
	margin: 0 auto;
	background: #f1f1f1;
	margin-top: 60px;
}
#about_title{
	height: 160px;
	line-height: 170px;
	color: #6ea4ca;
	font-size: 38px;
	text-align: center;
	position:relative;
}
#about_title span{
	height: 2px;
	display: block;;
	width: 40px;
	border-bottom: 2px solid #6EA4CA;
	position: absolute;
	left: 48%;
	top: 104px;
}
#about_text{
	height: 60px;
	line-height: 20px;
	text-align: center;
	font-size: 16px;
	color: #6ea4ca;
}
#about_list{
	height: 600px;
}
#about_list .about_list_div{
	height: 290px;
	width: 430px;
	float: left;
	margin-bottom: 5px;
}
.person{
	height: 120px;
	background: #0c3c60;
	}
.person_picture{
	width: 120px;
	height: 120px;
	overflow: hidden;
}
.person_picture img{
	width: 120px;
	height: 120px;
	overflow: hidden;
	display: block;
}
.person_name{
	color: #fff;
	font-size:22px ;
	padding-left:20px ;
	line-height: 38px;
	margin-top: 20px;
}
.person_job{
	color: #c4e0e2;
	font-size:24px ;
	padding-left:20px ;
	line-height: 34px;
}
.person_introduce{
	margin-top: 20px;
	padding:20px 5px ;
	line-height: 20px;
	font-size: 16px;
	color: #0c3c60;
	
}





