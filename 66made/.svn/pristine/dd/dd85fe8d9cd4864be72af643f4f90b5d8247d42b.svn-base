/*头部 导航公共样式部分*/
#header_box{
	width: 100%;
	height: 60px;
	background: #0c3c60;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 100;
}
#header{
	margin: 0 auto ;
	width: 980px;
	height: 60px;
	line-height: 60px;
	color: #fff;
	cursor: pointer;
}
#log a{
	color: #fff;
}
#log{
	font-size: 24px;
}
#log span{
	color: #7dc8df;
}
#nav a{
	color:#fff;
	font-size: 18px;
	margin-left: 40px;
}
#nav a:hover{
color: #d1e0eb
}
/*页尾公共样式部分*/
#footer_box{
	height: 100px;
	width: 100%;
	background: #0C3C60;
}
#footer{
	height: 60px;
	width: 900px;
	margin:0 auto;
	padding-top:40px ;
	padding-left: 80px;
	font-size: 16px;
	color: #fff;
}
#footer a{
	color: #fff;
}
#footer .footer_a2{
	text-decoration: underline;
}
#share span{
	display: block;
	width: 19px;
	height: 15px;
	float: left;
	margin-right: 15px;
	cursor: pointer;
	margin-top:3px
}
#share_weibo{
	background: url(../../images/model-1/background/share_bg.png) no-repeat;
}
#share_weixin{
	background: url(../../images/model-1/background/share_bg.png) -38px  no-repeat;
}
#share_qq{
	background: url(../../images/model-1/background/share_bg.png) -80px  no-repeat;
}
.about_list_div_1,.about_list_div_3{
	margin-right:120px ;
}

