
#banner_box{
	width: 100%;
	height: 638px;
	margin-top:60px;
	background: url(../../images/model-1/background/index_bg.png) no-repeat;;
	position: relative;
}
#banner{
	width: 100%;
	position: absolute;
	left: 0px;
	bottom: 0px;
	height: 140px;
}
#banner_bg{
	width: 100%;
	height: 140px;
	background: #fff;
	opacity: 0.5;
	filter: alpha(opacity=50);
	position: absolute;
	left: 0px;
	bottom: 0px;	
}
#banner_main{
	width: 980px;
	margin: 0 auto;
	text-align: center;
}

.banner_main_p1{
	font-size: 20px;
	line-height: 75px;
	color: #0c3c60;
	font-weight:600 ;
}
.banner_main_p2{
	font-size: 40px;
	line-height: 40px;
	color: #0c3c60;
	font-weight: 600;
}
#main_box{
	widows: 100%;
	height: 420px;
	background: #fff;
}
#main{
	width: 980px;
	height: 370px;
	margin: 0 auto;
	padding-top: 50px;
}

.main_small_box{
	width: 270px;
	height:300px ;
	background: #0c3c60;
	float: left;
	margin-right: 25px;
	color:#fff;
	padding: 25px 20px 0 20px;
	text-align: center;
	position: relative;
}
#main_clients{
	margin-right: 0;
}
.mian_title{
	line-height: 35px;
	font-size: 19px;
	margin-bottom: 20px;
}
.mian_article{
	font-size: 14px;
	line-height: 25px;
	height:140px ;
}
.main_small_box a{
	display: block;
	height: 40px;
	width: 140px;
	color: #FFF;
	line-height: 40px;
	background: #6ea4ca;
	font-size: 14px;
	position: absolute;
	left: 50%;
	top: 80%;
	margin-left: -70px;
}
#about_box{
	width: 100%;
	height: 550px;
	background-color: #d1e0eb;
}
#about{
	width: 820px;
	height: 550px;
	margin: 0 auto;
	padding: 0 80px;
	text-align: center;
	position: relative;
}
#about_title{
	height: 180px;
	line-height: 180px;
	font-size:40px;
	color: #0c3c60;
}
#about_article{
	line-height: 26px;
	font-size: 14px;
	color: #0c3c60;
}

#about_href{
	display: block;
	height: 40px;
	width: 140px;
	text-align: center;
	color: #fff;
	background-color: #0c3c60;
	line-height: 40px;
	font-size: 14px;	
	position: absolute;
	left: 50%;
	margin-left: -70px;
	bottom: 30px;
}
#contact_box{
	height: 705px;
	width: 100%;
	background: #0C3C60;
}
#contact{
	width: 980px;
	margin: 0 auto ;
}
#contact_title{
	height: 150px;
	line-height: 150px;
	color: #fff;
	font-size:40px ;
	text-align: center;
}
#contact_title_main{
	height: 500px;
}
#map_box {
	height: 425px;
	width: 485px;
	overflow: hidden;
}
#map_box img{
	display: block;
}
#contact_maseeges{
	height: 425px;
	width: 335px;
	position: relative;
}
#company_info{
	padding-top: 35px;
	height: 80px;
	color: #fff;
	font-size: 16px;
}

#user_info input{
	width: 100%;
	height: 30px;
	line-height: 30px;
	outline: none;
	border: none;
	background: #fff;
	margin-bottom: 10px;
	text-indent: 5px;
}
#user_info textarea{
	resize: none;
	width: 325px;
	height: 135px;
	padding: 5px;
	border: none;
	outline: none;
}
#user_send{
	width: 45px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	background: #6ea4ca;
	color: #fff;
	border: none;
	outline: none;
	position: absolute;
	right: 0;
	bottom: 0;
}

