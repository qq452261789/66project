.mobile_model{
    margin: 0 auto;
    margin-top: 100px;
    width:326px;
    height:750px;
    overflow:auto;
}
#about_box{
    margin-top: 35px;
    margin-bottom: 20px;
    width:100%;
    background-color: #fff;
}
#about_box .about_title{
    width:78%;
    margin:0 auto;
    text-align: center;
}
#about_box .about_title p{
    margin-bottom: 5px;
    font-size: 2rem;
    color:#6ea4ca;
}
#about_box .about_title div{
    margin:0 auto;
    margin-bottom: 30px;
    width: 30px;
    height: 3px;
    background-color: #6ea4ca;
}
#about_box .about_title p:nth-of-type(2){
    font-size: 1.4rem;
}
#about_box .about_list{
    width:100%;
    background-color: #fff;
}
#about_box .about_list li{
    margin:0 auto;
    margin-top: 50px;
    width:87.5%;  
}
#about_box .about_list li div{
    width:100%;
    background-color: #0c3c60;
}
#about_box .about_list li div div:nth-of-type(1){
    width: 40%;
}
#about_box .about_list li div div:nth-of-type(2){
    box-sizing: border-box;
    padding: 25px 0;
    width: 60%;
}
#about_box .about_list li div div:nth-of-type(2) p{
    margin-left: 8%;
    font-size: 2rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
#about_box .about_list li div div:nth-of-type(2) p:nth-of-type(1){
    color: #fff;
}
#about_box .about_list li div div:nth-of-type(2) p:nth-of-type(2){
    color: #c4e0e2;
}
#about_box .about_list li>p{
    margin-top: 30px;
    width: 100%;
    font-size: 1.4rem;
    color: #0c3c60;
    text-align: left;
}
/*滚动条样式*/
/*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/  
.mobile_model::-webkit-scrollbar  
{  
    width: 6px;  
    height: 100%;  
    background-color: #fff;  
}  
  
/*定义滚动条轨道 内阴影+圆角*/  
.mobile_model::-webkit-scrollbar-track  
{
    -webkit-box-shadow: inset 0 0 6px #fff;  
    border-radius: 10px;  
    background-color: #fff;  
}  
  
/*定义滑块 内阴影+圆角*/  
.mobile_model::-webkit-scrollbar-thumb  
{  
    border-radius: 10px;  
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
    background-color: #6ea4ca;  
}  