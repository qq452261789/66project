#service_content_box{
    width: 100%;
}
#book_online_content_box .book_online_content{
    width:980px;
    margin: 0 auto;
    text-align: center;
}
#book_online_content_box .book_online_content .book_online_title p{
    margin:0 auto;
    margin-top: 50px;
    width:550px;
    font-size: 20px;
    color: #2c2c2b;
}
#book_online_content_box .book_online_content .book_online_list{
    margin-top: 70px;
    width: 980px;
}
#book_online_content_box .book_online_content .book_online_list li{
    display:inline-block;
    width:300px;
    height:450px;
}
#book_online_content_box .book_online_content .book_online_list li>div{
    position: relative;
    height:350px;
    overflow:hidden;
}
#book_online_content_box .book_online_content .book_online_list li>div>div:nth-of-type(1){
    position:absolute;
    top: 0;
    left:0;
    width:100%;
    height:200px;
    overflow: hidden;
}
#book_online_content_box .book_online_content .book_online_list li>div>div:nth-of-type(2){
    position:absolute;
    top:200px;
    left:0;
    height:300px;
    padding:0 25px;
}
#book_online_content_box .book_online_content .book_online_list li>div>div:nth-of-type(2)>p:nth-of-type(1){
    margin-top: 40px;
    font-size: 16px;
    color:#544d4d;
}
#book_online_content_box .book_online_content .book_online_list li>div>div:nth-of-type(2)>p:nth-of-type(2){
    margin-top: 15px;
    font-size: 16px;
    color:#544d4d;
}
#book_online_content_box .book_online_content .book_online_list li>div>div:nth-of-type(2)>p:nth-of-type(3){
    margin-top: 45px;
    font-size: 14px;
    color:#544d4d;
    opacity:0;
}
#book_online_content_box .book_online_content .book_online_list li button{
    margin-top: 10px;
    padding:10px 0;
    width:120px; 
    border:none;
    outline: none;
    background-color: #6ea4ca;
    font-size: 16px;
    font-family:"Microsoft Yahei";
    color:#fff;
   
}

/*滚动条样式*/
/*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/  
.book_online_list_text::-webkit-scrollbar  
{  
    width: 6px;  
    height: 100%;  
    background-color: #fff;  
}  
  
/*定义滚动条轨道 内阴影+圆角*/  
.book_online_list_text::-webkit-scrollbar-track  
{  
    -webkit-box-shadow: inset 0 0 6px #fff;  
    border-radius: 10px;  
    background-color: #fff;  
}  
  
/*定义滑块 内阴影+圆角*/  
.book_online_list_text::-webkit-scrollbar-thumb  
{  
    border-radius: 10px;  
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
    background-color: #6ea4ca;  
}  