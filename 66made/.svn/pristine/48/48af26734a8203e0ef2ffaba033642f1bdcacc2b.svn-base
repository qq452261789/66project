body{
	min-width: 980px;
}
#banner_box{
	position: relative;
	margin-top:90px;
	width: 100%;
	height: 640px;
	background: url(../../images/model-1/background/index_bg.png) no-repeat;
	background-position: top center;
	background-size: 1920px 640px;
}
#banner{
	width: 100%;
	position: absolute;
	left: 0px;
	bottom: 0px;
	height: 140px;
}
#banner_bg{
	width: 100%;
	height: 140px;
	background: #fff;
	opacity: 0.5;
	filter: alpha(opacity=50);
	position: absolute;
	left: 0px;
	bottom: 0px;	
}
#banner_main{
	width: 980px;
	margin: 0 auto;
	text-align: center;
}

.banner_main_p1{
	font-size: 20px;
	margin-top: 25px;
	color: #0c3c60;
	font-weight:600 ;
}
.banner_main_p2{
	font-size: 40px;
	line-height: 40px;
	color: #0c3c60;
	font-weight: 600;
	margin-top: 20px;
}
#main_box{
	widows: 100%;
	height: 420px;
	background: #fff;
}
#main{
	width: 980px;
	height: 370px;
	margin: 0 auto;
	padding-top: 50px;
}

.main_small_box{
	width: 270px;
	height:300px ;
	background: #0c3c60;
	float: left;
	margin-right: 25px;
	color:#fff;
	padding: 25px 20px 0 20px;
	text-align: center;
	position: relative;
}
#main_clients{
	margin-right: 0;
}
.mian_title{
	font-size: 19px;
	margin-bottom: 20px;
}
.mian_article{
	font-size: 14px;
	line-height: 20px;
	height: 180px;
	overflow-y:auto ;
}
.main_small_box a{
	display: block;
	height: 40px;
	width: 140px;
	color: #FFF;
	line-height: 40px;
	background: #6ea4ca;
	font-size: 14px;
	position: absolute;
	left: 50%;
	bottom: 15px;
	margin-left: -70px;
}
.main_small_box a:hover{
	background: #3b729b;
}
#about_box{
	width: 100%;
	height: 600px;
	background-color: #d1e0eb;
}
#about{
	width: 820px;
	height: 600px;
	margin: 0 auto;
	padding: 0 80px;
	text-align: center;
	position: relative;
}
#about_title{
	padding-top: 45px;
	margin-bottom: 30px;
	font-size:40px;
	color: #0c3c60;
}
#about_article{
	line-height: 26px;
	font-size: 14px;
	color: #0c3c60;
	margin-bottom: 30px;
}

#about_href{
	display: block;
	height: 40px;
	width: 140px;
	text-align: center;
	color: #fff;
	background-color: #0c3c60;
	line-height: 40px;
	font-size: 14px;	
	margin:0 auto;
	
}
#about_href:hover{
	background:  #3b729b;
}
#contact_box{
	height: 820px;
	width: 100%;
	background: #0C3C60;
}
#contact{
	width: 980px;
	margin: 0 auto ;
}
#contact_title{
	padding-top: 80px;
	padding-bottom:130px ;
	color: #fff;
	font-size:40px ;
	text-align: center;
}
#contact_title_main{
	height: 500px;
}
.map_box {
	height: 425px;
	width: 485px;
	overflow: hidden;
}
.map_box img{
	display: block;
}
.contact_maseeges{
	height: 425px;
	width: 335px;
	position: relative;
}
.company_info{
	padding-top: 35px;
	height: 80px;
	color: #fff;
	font-size: 16px;
}

.user_info input{
	width: 100%;
	height: 30px;
	line-height: 30px;
	outline: none;
	border: none;
	background: #fff;
	margin-bottom: 10px;
	text-indent: 5px;
	font-size: 14px;
	font-family: "微软雅黑";
	color: #1f3a4f;
}
.user_info textarea{
	resize: none;
	width: 325px;
	height: 135px;
	padding: 5px;
	border: none;
	outline: none;
	font-size: 14px;
	font-family: "微软雅黑";
}
.user_send{
	width: 45px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	background: #6ea4ca;
	color: #fff;
	border: none;
	outline: none;
	position: absolute;
	right: 0;
	bottom: 0;
	font-family:"微软雅黑";
	cursor: pointer;
}

